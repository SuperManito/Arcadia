{
  "name": "arcadia-website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start --no-minify",
    "dev": "docusaurus start --no-minify",
    "build": "rimraf build && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "lint": "eslint ./src --ext .ts,tsx,js,jsx,mjs,mdx",
    "lint:fix": "eslint --fix ./src --ext .ts,tsx,js,jsx,mjs,mdx",
    "clean:cache": "rimraf .docusaurus && rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:build": "rimraf build",
    "clean:lock": "rimraf yarn.lock && rimraf pnpm-lock.yaml && rimraf package.lock.json",
    "clean:lib": "rimraf node_modules",
    "clean": "pnpm clean:build && pnpm clean:lock && pnpm clean:lib",
    "prepare": "husky",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids"
  },
  "dependencies": {
    "@algolia/client-search": "^5.24.0",
    "@docusaurus/core": "^3.7.0",
    "@docusaurus/faster": "^3.7.0",
    "@docusaurus/plugin-client-redirects": "^3.7.0",
    "@docusaurus/preset-classic": "^3.7.0",
    "@docusaurus/remark-plugin-npm2yarn": "^3.7.0",
    "@docusaurus/theme-classic": "^3.7.0",
    "@docusaurus/theme-common": "^3.7.0",
    "@docusaurus/utils": "^3.7.0",
    "@docusaurus/utils-common": "^3.7.0",
    "@iconify/react": "^6.0.0",
    "@mdx-js/react": "^3.1.0",
    "antd": "^5.24.9",
    "clsx": "^2.1.1",
    "docusaurus-plugin-image-zoom": "^3.0.1",
    "mini-svg-data-uri": "^1.4.4",
    "motion": "^12.9.2",
    "postcss": "^8.5.3",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@docusaurus/eslint-plugin": "^3.7.0",
    "@docusaurus/module-type-aliases": "^3.7.0",
    "@docusaurus/plugin-content-docs": "^3.7.0",
    "@docusaurus/plugin-ideal-image": "^3.7.0",
    "@docusaurus/tsconfig": "^3.7.0",
    "@docusaurus/types": "^3.7.0",
    "@tailwindcss/postcss": "^4.1.5",
    "@tsconfig/docusaurus": "^2.0.3",
    "@types/node": "^22.15.3",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.4.0",
    "eslint": "^8.57.0",
    "eslint-config-love": "^47.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.2.0",
    "eslint-plugin-react": "^7.37.5",
    "husky": "^9.1.7",
    "prismjs": "^1.30.0",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.5",
    "typescript": "~5.8.3"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=20.0"
  },
  "lint-staged": {
    "*.{.ts,tsx,js,jsx,mjs,mdx}": [
      "pnpm lint:fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  }
}
